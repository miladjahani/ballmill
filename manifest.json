  "related_applications": [],
  
  "prefer_related_applications": false,
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "handle_links": "preferred",
  
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  
  "protocol_handlers": [
    {
      "protocol": "web+ballmill",
      "url": "/?calculation=%s"
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text", 
      "url": "url",
      "files": [
        {
          "name": "calculation_file",
          "accept": [".json", ".csv", ".txt"]
        }
      ]
    }
  },
  
  "file_handlers": [
    {
      "action": "/open-calculation",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"],
        "text/plain": [".txt"]
      }
    }
  ]
}